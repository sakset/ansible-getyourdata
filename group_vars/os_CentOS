# nginx
nginx_user: nginx
nginx_default_config_path: /etc/nginx/conf.d/default.conf
nginx_config_path: /etc/nginx/nginx.conf

# postgresql
postgresql_package_name: postgresql-server
libpq_dev_name: postgresql-devel

# redis
redis_package_name: redis
redis_config_path: /etc/redis.conf

# redis-cache
redis_cache_config_path: /etc/redis-cache.conf

# uwsgi
uwsgi_executable_path: /usr/bin/uwsgi
python_dev_package_name: python-devel
virtualenv_package_name: python-virtualenv
